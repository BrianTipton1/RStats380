library(ggplot2)
library(dplyr)

catXWithString <- function(string,
                           x,
                           nl = TRUE,
                           sep = ' ')
{
  if (nl) {
    cat(paste(string, toString(x), "\n", sep = sep))
    
  }
  else {
    cat(paste(string, toString(x), sep = sep))
    
  }
}
